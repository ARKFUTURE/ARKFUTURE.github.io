        list module 'blockuser'
        list module 'certauth'
        list module 'fail2ban'
        list module 'identfile'
        list module 'lastseen'
        list module 'notify_connect'
        # list module 'imapauth'
        
config listener 'Default'
        option allowirc 'true'
        option allowweb 'true'
        option host '0.0.0.0'
        option port '12345'
        option ipv4 'true'
        option ipv6 'true'
        option ssl 'false'

